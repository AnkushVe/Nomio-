{
  "name": "travel-planner",
  "version": "1.0.0",
  "description": "AI-powered travel planner with itinerary generation",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "node setup.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^159.0.0",
    "nodemailer": "^7.0.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "travel",
    "planner",
    "ai",
    "itinerary",
    "react"
  ],
  "author": "Travel Planner Team",
  "license": "MIT"
}
