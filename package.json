{
  "name": "nomio-travel-planner",
  "version": "1.0.0",
  "description": "AI-powered travel planner with multi-agent system, Google Maps integration, and personalized itinerary generation",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/server.js",
    "vercel-build": "cd client && npm run build",
    "postinstall": "cd client && npm install",
    "preview": "npm run build && npm start"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^159.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6",
    "twilio": "^5.9.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "travel",
    "planner",
    "ai",
    "itinerary",
    "react",
    "google-maps",
    "multi-agent",
    "nomio",
    "travel-assistant",
    "gemini-ai"
  ],
  "author": "AnkushVe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnkushVe/Nomio-.git"
  },
  "homepage": "https://nomio-vqoy.vercel.app"
}
